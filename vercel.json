{
  "version": 2,
  "builds": [
    {
      "src": "src/server.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["src/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/server.ts"
    }
  ],
  "crons": [
    {
      "path": "/src/jobs/vulnerabilityJob",
      "schedule": "45 1 * * *"
    }
  ],
  "buildCommand": "pnpm build"
}
